select atomno=7
colour atoms [143,143,255]
select (atomno>=8) and (atomno<=10)
colour atoms [255,255,255]
select all
spacefill off

# Bonds
wireframe on

# Ribbons
ribbons off

# Backbone
backbone off

# Labels
labels off

# Monitors
monitors off

# ssbonds
ssbonds off

# hbonds
hbonds off

# Dots
set solvent false
set radius 0
set dots 1
dots off

# AtomSets
# no sets

select all

rotate world

